<h2>Create a New Place</h2>
<form action="/main" method="GET">
    <div class="form-group">
        <label for="name">Place Name: </label>
        <input type="string" class="form-control" id="name">
    </div>
    <div class="form-group">
        <label for="streetNumber">Street Number: </label>
        <input type="number" class="form-control" id="streetNumber">
    </div>
    <div class="form-group">
        <label for="streetName">Street Name: </label>
        <input type="string" class="form-control" id="streetName">
    </div>
    <div class="form-group">
        <label for="cityName">City: </label>
        <input type="string" class="form-control" id="cityName">
    </div>
    <div class="form-group">
        <label for="stateName">State: </label>
        <input type="string" class="form-control" id="stateName">
    </div>
    <div class="form-group">
        <label for="zipCode">Zip Code: </label>
        <input type="number" class="form-control" id="zipCode">
    </div>
    <div class="form-group">
        <label for="category">Category: </label>
        <input type="string" class="form-control" id="category">
    </div>
    <div class="form-group">
        <label for="review">Review: </label>
        <input type="text" class="form-control" id="review">
    </div>
    <input type="submit">
</form>