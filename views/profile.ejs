<div>
    <h2 class="d-flex justify-content-center text-light">Here's a list of your saved places!</h3>
        <div class="d-flex justify-content-center">
            <% foundData.forEach((item) => { %>
                <div class="card m-2 p-3 bg-dark text-light">
                <h5 class="card-title">Place Name: <%= item.name %> </h5>
                <h6 class="card-subtitle mb-2 text-muted">Category: <%= item.category %> </h6>
                <p class="card-text">Review: <%= item.review %> </p>
                <p> <small> <%= item.createdAt %> </small> </p>
                <p> <small> <%= item.placeName%> </small> </p>
                <div class="container-fluid d-flex justify-content-around mb-2">
                    <a href="/comment/<%= item.id %>"><button class="btn btn-outline-info">Comment</button></a>
                    <a href="/profile/edit/<%= item.id %>"><button class="btn btn-outline-info">Edit</button></a>
                    <form action="/profile/<%= item.id %>/?_method=DELETE" method="POST">
                        <input type="submit" value="Delete" class="btn btn-outline-danger">
                    </form>
                </div>
                </div>    
            <% }) %>
        </div>
</div>





